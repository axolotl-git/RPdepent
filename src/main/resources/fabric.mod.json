{
  "schemaVersion": 1,
  "id": "rpdepent",
  "version": "${version}",
  "name": "RPdepent",
  "description": "a mod for checking the dependencies of a resourcepack",
  "authors": ["Axolotl/Axolotl.cpp"],
  "contact": {},
  "license": "GPL-3.0",
  "icon": "assets/rpdepent/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "axo.dev.rpdepent.client.RpdepentDataGenerator"
    ],
    "client": [
      "axo.dev.rpdepent.client.RpdepentClient"
    ],
    "main": [
      "axo.dev.rpdepent.Rpdepent"
    ]
  },
  "mixins": [
    "rpdepent.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.17",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
